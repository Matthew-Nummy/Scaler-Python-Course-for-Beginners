def star_pattern(n):
    '''input: n = An Integer value defines number of rows
       output: print triangle star pattern'''
    # YOUR CODE GOES HERE
    for i in range(1, n + 1):
        for e in range(n - i):
            print(' ', end = "")
        for s in range(1, 2 * i):
            print('*', end = "")
            
        print()
